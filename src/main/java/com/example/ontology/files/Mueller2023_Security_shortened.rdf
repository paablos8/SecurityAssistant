        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#EmergencyPlanningControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#MaintenanceContractControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SecurityTrainingControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ThirdPartyServiceManagementControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#BusinessContinuityPlanAndStrategyControl"/>
