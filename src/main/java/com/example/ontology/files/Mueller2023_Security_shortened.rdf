        <secontMueller:control xml:lang="en">Access to information and application system functions by users and support personnel should be restricted in accordance with the defined access control policy.</secontMueller:control>
        <rdfs:label xml:lang="en">A.11.6.1 Information access restriction</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_6_2 -->

