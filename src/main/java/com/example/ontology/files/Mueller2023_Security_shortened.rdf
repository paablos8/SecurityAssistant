        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_11_5"/>
        <secontMueller:control xml:lang="en">Inactive sessions should shut down after a defined period of inactivity.</secontMueller:control>
        <secontMueller:otherInformation xml:lang="en">Sessions should be shut down to prevent denial of service attacks or unauthorized access, especially in locations that include public or external areas outside the organization&apos;s security management.</secontMueller:otherInformation>
        <rdfs:label xml:lang="en">A.11.5.5 Session time-out</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_5_6 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_11_5_6">
