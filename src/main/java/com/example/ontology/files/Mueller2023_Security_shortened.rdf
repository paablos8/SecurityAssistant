        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_12_5"/>
        <secontMueller:control xml:lang="en">Modifications to software packages should be discouraged, limited to necessary changes, and all changes should be strictly controlled.</secontMueller:control>
        <rdfs:label xml:lang="en">A.12.5.3 Restrictions on changes to software packages</rdfs:label>
    </owl:NamedIndividual>
    


