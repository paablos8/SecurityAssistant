        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#PositionResponsibilityControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SecurityTrainingControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#BusinessContinuityPlanningFrameworkControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#ChangeManagementControl"/>
