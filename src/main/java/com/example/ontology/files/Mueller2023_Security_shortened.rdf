        <rdfs:label xml:lang="en">A.11.2.2 Privilege management</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_2_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_11_2_3">
