    <!-- http://securityontology.sba-research.org/securityontology.owl#6_5_InternetAccessRisks -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#6_5_InternetAccessRisks">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#WKOITSicherheitshandbuch"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#PCAndInternetRegulationControl"/>
