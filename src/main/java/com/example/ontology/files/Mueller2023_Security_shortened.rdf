        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_12_2"/>
        <secontMueller:control xml:lang="en">Data input to applications should be validated to ensure that this data is correct and appropriate.</secontMueller:control>
        <secontMueller:otherInformation xml:lang="en">Where applicable: Automatic validation of input data can be considered to reduce errors and prevent attacks (e.g. buffer overflow attack, code injection).</secontMueller:otherInformation>
        <rdfs:label xml:lang="en">A.12.2.1 Input data validation</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_12_2_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_12_2_2">
