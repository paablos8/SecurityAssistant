        <standardControl_hasChild_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_12_3_2"/>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_12"/>
        <secontMueller:objective xml:lang="en">To protect the confidentiality, authenticity or integrity of information by cryptographic means.</secontMueller:objective>
