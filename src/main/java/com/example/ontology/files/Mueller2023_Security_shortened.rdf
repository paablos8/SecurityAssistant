    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_3_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_11_3_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
