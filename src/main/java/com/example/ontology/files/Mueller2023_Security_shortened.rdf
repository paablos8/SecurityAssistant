        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SessionTimeControl"/>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_11_5"/>
        <secontMueller:control xml:lang="en">Restrictions on connection times should be used to provide additional security for high-risk applications.</secontMueller:control>
        <secontMueller:otherInformation xml:lang="en">The limitation of session time reduces the risk of unauthorized access. It also prevents users from holding sessions open that want to avoid re-authentication.</secontMueller:otherInformation>
        <rdfs:label xml:lang="en">A.11.5.6 Limitation of connection time</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_6 -->

