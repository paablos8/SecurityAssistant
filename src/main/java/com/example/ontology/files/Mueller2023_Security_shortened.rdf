        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SecurityIncidentsProceduresControl"/>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_13_2"/>
        <secontMueller:control xml:lang="en">Management responsibilities and procedures should be established to ensure a quick, effective, and orderly response to information security incidents.</secontMueller:control>
        <secontMueller:otherInformation xml:lang="en">Information security incidents could spread across multiple organizations and even nations - sharing information about incidents should therefore be considered.</secontMueller:otherInformation>
        <rdfs:label xml:lang="en">A.13.2.1 Responsibilities and procedures</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_13_2_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_13_2_2">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#InformationSecurityPolicyControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SecurityIncidentsProceduresControl"/>
