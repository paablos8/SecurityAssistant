        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#TechnicalVulnerabilitiesControl"/>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_12_5"/>
        <secontMueller:control xml:lang="en">When operating systems are changed, business critical applications should be reviewed and tested to ensure there is no adverse impact on organizational operations or security.</secontMueller:control>
        <rdfs:label xml:lang="en">A.12.5.2 Technical review of applications after operating system changes</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_12_5_3 -->

