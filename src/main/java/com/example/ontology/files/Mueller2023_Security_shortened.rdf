        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_13"/>
        <secontMueller:objective xml:lang="en">To ensure a consistent and effective approach is applied to the management of information security incidents.</secontMueller:objective>
        <rdfs:label xml:lang="en">A.13.2 Management of information security incidents and improvements</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_13_2_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_13_2_1">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#BusinessContinuityPlanAndStrategyControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#ContactWithAuthoritiesControl"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#MaliciousCodeProtectionProceduresControl"/>
