    <!-- http://securityontology.sba-research.org/securityontology.owl#DataSecrecyControl -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#DataSecrecyControl">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#Control"/>
        <control_correspondsTo_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_6_1_5"/>
