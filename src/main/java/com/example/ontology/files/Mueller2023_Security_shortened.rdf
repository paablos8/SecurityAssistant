    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_4_2 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_11_4_2">
