        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#DataBreachNotificationControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#EmergencyRecoveryControl"/>
