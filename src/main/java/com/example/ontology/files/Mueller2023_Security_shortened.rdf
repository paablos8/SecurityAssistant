        <rdfs:label xml:lang="en">A.11.4.5 Segregation in networks</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_4_6 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_11_4_6">
