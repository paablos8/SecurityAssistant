        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#ProcessingRegulationControl"/>
        <standardControl_hasParent_StandardControl rdf:resource="http://securityontology.sba-research.org/securityontology.owl#A_12_2"/>
        <secontMueller:control xml:lang="en">Requirements for ensuring authenticity and protecting message integrity in applications should be identified, and appropriate controls identified and implemented.</secontMueller:control>
        <secontMueller:otherInformation xml:lang="en">Cryptographic techniques could provide message authentication.</secontMueller:otherInformation>
        <rdfs:label xml:lang="en">A.12.2.3 Message integrity</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_12_2_4 -->

