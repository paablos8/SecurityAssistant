        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.sba-research.org/securityontology.owl#SystemMonitoringRegulationControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#InformationLeakageControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#SystemCapacityMonitoringControl"/>
