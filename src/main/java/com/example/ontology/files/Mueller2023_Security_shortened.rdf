    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#5_8_2_VirusAvoidanceAndRecognitionByTheUser">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#WKOITSicherheitshandbuch"/>
