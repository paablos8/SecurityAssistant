        <secontMueller:objective xml:lang="en">To prevent unauthorized access to information held in application systems.</secontMueller:objective>
        <rdfs:label xml:lang="en">A.11.6 Application and information access control</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_11_6_1 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_11_6_1">
