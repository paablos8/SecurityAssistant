        <secontMueller:otherInformation xml:lang="en">For system and acceptance testing there are usually huge volumens of test data required that simulate operation data as well as possible.</secontMueller:otherInformation>
        <rdfs:label xml:lang="en">A.12.4.2 Protection of system test data</rdfs:label>
    </owl:NamedIndividual>
    


    <!-- http://securityontology.sba-research.org/securityontology.owl#A_12_4_3 -->

    <owl:NamedIndividual rdf:about="http://securityontology.sba-research.org/securityontology.owl#A_12_4_3">
        <rdf:type rdf:resource="http://securityontology.sba-research.org/securityontology.owl#ISO27001"/>
        <secontMueller:standardControl_relatedTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#ChangeManagementControl"/>
        <standardControl_correspondsTo_Control rdf:resource="http://securityontology.com/secontMueller.owl#FacilityEnvironmentSeparationLevelControl"/>
